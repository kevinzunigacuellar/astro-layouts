{
  "name": "astro-layouts",
  "type": "module",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinzunigacuellar/astro-layouts"
  },
  "bugs": {
    "url": "https://github.com/kevinzunigacuellar/astro-layouts/issues"
  },
  "homepage": "https://github.com/kevinzunigacuellar/astro-layouts#README",
  "description": "remark plugin to add folder-based layouts to MD and MDX files in Astro",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "vitest",
    "test": "vitest run"
  },
  "keywords": [
    "astro",
    "withastro",
    "astro-component"
  ],
  "author": {
    "name": "Kevin Zuniga Cuellar",
    "url": "https://twitter.com/kevinzunigacuel"
  },
  "license": "MIT",
  "devDependencies": {
    "@astrojs/markdown-remark": "^2.0.0",
    "astro": "^2.0.1",
    "tsup": "^6.5.0",
    "vitest": "^0.28.2"
  },
  "peerDependencies": {
    "astro": "^2.0.1"
  },
  "dependencies": {
    "picomatch": "^2.3.1"
  }
}
