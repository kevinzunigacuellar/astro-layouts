{
  "name": "astro-layouts",
  "type": "module",
  "version": "0.0.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinzunigacuellar/astro-layouts"
  },
  "bugs": {
    "url": "https://github.com/kevinzunigacuellar/astro-layouts/issues"
  },
  "homepage": "https://github.com/kevinzunigacuellar/astro-layouts#README",
  "description": "remark plugin to add folder-based layouts to MD and MDX files in Astro",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --watch --format esm",
    "test": "vitest run"
  },
  "keywords": [
    "astro",
    "withastro",
    "astro-component"
  ],
  "author": {
    "name": "Kevin Zuniga Cuellar",
    "url": "https://twitter.com/kevinzunigacuel"
  },
  "license": "MIT",
  "devDependencies": {
    "@astrojs/markdown-remark": "^1.1.3",
    "astro": "^1.6.7",
    "tsup": "^6.5.0",
    "vitest": "^0.25.2"
  },
  "peerDependencies": {
    "astro": "^1.6.7"
  },
  "dependencies": {
    "micromatch": "^4.0.5"
  }
}
